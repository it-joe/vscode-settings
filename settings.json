{
    "editor.fontLigatures": true,
    "editor.fontFamily": "JetBrainsMono Nerd Font Light",
    "terminal.integrated.fontFamily": "JetBrainsMono Nerd Font Light",
    "editor.cursorStyle": "block",
    "workbench.startupEditor": "newUntitledFile",
    "workbench.editor.highlightModifiedTabs": true,
    "workbench.iconTheme": "bearded-icons",
    "workbench.tree.indent": 20,
    "window.restoreWindows": "none",
    "window.title": "${dirty}${activeEditorLong}${separator}${rootName}${separator}${appName}",
    "window.titleBarStyle": "custom",
    "explorer.openEditors.visible": 1,
    "explorer.compactFolders": false,
    "files.autoGuessEncoding": true,
    "files.trimFinalNewlines": true,
    "breadcrumbs.enabled": true,
    "extensions.autoUpdate": true,
    "extensions.ignoreRecommendations": false,
    "git.ignoreMissingGitWarning": true,
    "git.autofetch": true,
    "security.workspace.trust.untrustedFiles": "open",
    "security.workspace.trust.banner": "never",
    "telemetry.telemetryLevel": "off",
    "editor.accessibilitySupport": "off",
    "powershell.integratedConsole.showOnStartup": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    // "workbench.colorCustomizations": {
    //     "[Theme]": {
    //     }
    // },
    // "editor.tokenColorCustomizations": {
    //     "[Theme]": {
    //         "textMateRules": [
    //             {
    //                 "scope": "comment",
    //                 "settings": {
    //                     "foreground": "#636e7b"
    //                 }
    //             }
    //         ]
    //     }
    // },
    "powershell.promptToUpdatePowerShell": false,
    "terminal.integrated.tabs.enabled": false,
    "todohighlight.isEnable": true,
    "todohighlight.include": [
        "**/*.ps1",
        "**/*.js",
        "**/*.jsx",
        "**/*.ts",
        "**/*.tsx",
        "**/*.html",
        "**/*.php",
        "**/*.css",
        "**/*.scss"
    ],
    "codesnap.showWindowControls": false,
    "codesnap.shutterAction": "copy",
    "codesnap.backgroundColor": "#ffffff",
    "codesnap.transparentBackground": true,
    "codesnap.boxShadow": "rgba(0, 0, 0, 0.55) 0px 10px 10px",
    "editor.stickyScroll.enabled": true,
    "terminal.integrated.stickyScroll.enabled": true,
    "workbench.tree.enableStickyScroll": true,
    "files.defaultLanguage": "PowerShell",
    "files.encoding": "utf8bom",
    "powershell.codeFormatting.autoCorrectAliases": true,
    "powershell.codeFormatting.useCorrectCasing": true,
    "editor.formatOnSave": true,
    "powershell.integratedConsole.focusConsoleOnExecute": false,
    "editor.detectIndentation": false,
    "git.suggestSmartCommit": false,
    "editor.lineHeight": 1.5,
    "git.ignoreRebaseWarning": true,
    "workbench.colorTheme": "Bearded Theme Solarized"
}